  pod 'Sundial', :path => '.', :inhibit_warnings => false
  pod 'SnapKit'

  pod 'Astrolabe', :git => 'git@github.com:netcosports/Astrolabe.git', :branch => 'kmm'
  
  pod 'PinLayout'
end
